{
  "name": "node-koa-demo",
  "version": "1.0.0",
  "description": "node koa demo",
  "main": "index.js",
  "scripts": {
    "start": "nodemon",
    "dev": "rm -rf dist && cross-env NODE_ENV=development ts-node ./src/index.ts",
    "dev-old": "nodemon --watch src/**/* -e ts,tsx --exec ts-node ./src/index.ts",
    "build-server": "rm -rf dist && tsc -p ./tsconfig.json --moduleResolution node",
    "build-front": "rm -rf built && cross-env NODE_ENV=production webpack --env=production --mode=production --config config/webpack.dev.js",
    "build": "npm run build-front"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyenToretto/kjf_node.git"
  },
  "keywords": [
    "node",
    "ts",
    "koa",
    "angular2"
  ],
  "author": "RyenToretto",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RyenToretto/kjf_node/issues"
  },
  "homepage": "https://github.com/RyenToretto/kjf_node#readme",
  "dependencies": {
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-ejs": "^4.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@types/mysql": "^2.15.15",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.41",
    "@types/node": "^12.0.10",
    "@types/webpack-dev-middleware": "^2.0.3",
    "autoprefixer": "^9.6.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.10",
    "file-loader": "^4.0.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-harddisk-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0"
  }
}
